<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:clickable="true"
    android:paddingBottom="4dp"
    android:background="@color/bg_gray">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

    <LinearLayout
        android:paddingTop="15dp"
        android:background="#ffffff"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_height="wrap_content">

        <RelativeLayout

            android:id="@+id/relative_tjq"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content">

            <ImageView

                android:layout_centerHorizontal="true"
                android:id="@+id/iv_main_second"
                android:layout_width="@dimen/iv_main_second"
                android:layout_height="@dimen/iv_main_second"
                android:scaleType="fitXY"
                android:background="@mipmap/tjq"
                />
            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/iv_main_second"
                android:text="埃孚森"
                android:textSize="@dimen/textsize_small"
                android:textColor="@color/shouye_renmen_tv"
                android:layout_centerHorizontal="true"
                />
        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/relative_zpjy"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_centerHorizontal="true"
                android:id="@+id/iv_main_second2"
                android:layout_width="@dimen/iv_main_second"
                android:layout_height="@dimen/iv_main_second"
                android:scaleType="fitXY"
                android:background="@mipmap/zpby"
                />
            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/iv_main_second2"
                android:text="4S店"
                android:textSize="@dimen/textsize_small"
                android:textColor="@color/shouye_renmen_tv"
                android:layout_centerHorizontal="true"
                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relative_qcjy"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/iv_main_second3"
                android:layout_centerHorizontal="true"
                android:layout_width="@dimen/iv_main_second"
                android:layout_height="@dimen/iv_main_second"
                android:scaleType="fitXY"
                android:background="@mipmap/qcjy"
                />
            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/iv_main_second3"
                android:text="二手车"
                android:textSize="@dimen/textsize_small"
                android:textColor="@color/shouye_renmen_tv"
                android:layout_centerHorizontal="true"
                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relative_xiuc"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content">

