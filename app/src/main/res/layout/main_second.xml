                android:layout_below="@id/iv_main_second4"
                android:text="车辆租赁"
                android:textColor="@color/shouye_renmen_tv"
                android:textSize="@dimen/textsize_small"
                android:layout_centerHorizontal="true"
                />
        </RelativeLayout>

    </LinearLayout>


    <LinearLayout
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:background="#ffffff"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:id="@+id/relative_xic"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_centerHorizontal="true"
                android:id="@+id/iv_main_second5"
                android:layout_width="@dimen/iv_main_second"
                android:layout_height="@dimen/iv_main_second"
